{
  "name": "GitHub Light Navbar",
  "version": "1.0.0",
  "description": "Brings back the old navbar.",
  "homepage_url": "https://github.com/stefansundin/github-light-navbar",
  "author": "Stefan Sundin",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "js": ["github.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
